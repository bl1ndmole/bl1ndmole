#include <vector>
#include <iostream>
#include <gtest/gtest.h>
using namespace std;
int vec(vector<int> v, int n) {
    for (int i : v) {
        i += n;
        cout << i << ' ';
    }
    return 0;
}
int main() {
    vector<int> v;
    v.push_back(10);
    v.push_back(20);
    v.push_back(30);
    int n = 5;
    vec(v, n);
}
